{
  "name": "ember-intercom-io",
  "version": "0.3.0",
  "description": "Idiomatic, deep Intercom.io integration for ember.js apps",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember server",
    "test": "ember try:each",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "repository": "https://github.com/mike-north/ember-intercom-io",
  "engines": {
    "node": "6.* || >= 7.*"
  },
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/travis-cli": "7.2.1",
    "@types/ember": "^2.8.4",
    "broccoli-asset-rev": "^2.4.5",
    "ember-cli": "~3.0.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "~4.2.3",
    "ember-cli-fastboot": "^1.1.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.0.0",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^4.1.0",
    "ember-sinon": "1.0.1",
    "ember-sinon-qunit": "2.1.0",
    "ember-source": "~3.0.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "greenkeeper-lockfile": "^1.2.1",
    "loader.js": "^4.0.10"
  },
  "optionalDependencies": {
    "@mike-north/js-lib-renovate-config": "^0.0.1",
    "@mike-north/js-lib-semantic-release-config": "^0.0.0-development",
    "semantic-release": "^15.9.12"
  },
  "keywords": [
    "ember-addon",
    "intercom",
    "chat"
  ],
  "dependencies": {
    "ember-cli-babel": "^6.16.0",
    "ember-cli-htmlbars": "^2.0.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
