{
  "name": "ember-intercom-io",
  "version": "0.0.0-development",
  "description": "Idiomatic, deep Intercom.io integration for ember.js apps",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember server",
    "test": "ember test",
    "test:all": "ember try:each",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "devDependencies": {
    "@commitlint/cli": "7.3.2",
    "@commitlint/config-conventional": "7.3.1",
    "@commitlint/travis-cli": "7.3.2",
    "@ember/optional-features": "0.7.0",
    "@mike-north/js-lib-renovate-config": "1.1.5",
    "@mike-north/js-lib-semantic-release-config": "1.0.1",
    "@types/ember": "3.0.26",
    "@types/ember-qunit": "3.4.5",
    "@types/ember__test-helpers": "0.7.7",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "3.7.1",
    "ember-cli-dependency-checker": "3.1.0",
    "ember-cli-eslint": "5.1.0",
    "ember-cli-fastboot": "2.0.3",
    "ember-cli-htmlbars-inline-precompile": "2.1.0",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-template-lint": "1.0.0-beta.2",
    "ember-cli-uglify": "2.1.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.0",
    "ember-load-initializers": "2.0.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "4.2.0",
    "ember-resolver": "5.0.1",
    "ember-sinon": "3.1.0",
    "ember-sinon-qunit": "3.4.0",
    "ember-source": "3.7.0",
    "ember-source-channel-url": "1.1.0",
    "ember-try": "1.1.0",
    "eslint-plugin-ember": "6.1.0",
    "eslint-plugin-node": "8.0.1",
    "husky": "1.2.1",
    "loader.js": "4.7.0",
    "qunit-dom": "0.8.4",
    "semantic-release": "15.12.4"
  },
  "keywords": [
    "ember-addon",
    "intercom",
    "chat"
  ],
  "repository": "https://github.com/mike-north/ember-intercom-io",
  "license": "MIT",
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "ember-cli-babel": "^7.1.3",
    "ember-cli-htmlbars": "^3.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "extends": "@mike-north/js-lib-semantic-release-config"
  }
}
